{
  "pacman": {
    "installdir": "/services/{{name}}/frontend",
    "content": {
      "${installdir}/": {
        "base": "public"
      },
      "/etc/nginx/config.d/${name}.conf": "pacman/nginx.conf"
    },
    "depends": {
      "nginx-mainline": ">=1.17.8"
    }
  },
  "template": {
    "usedBy": [ "arlac77/npm-package-template-kronos-app" ]
  }
}
